    <div class="container-fluid-1" style="height: 939px;">
      <%- include ('../partials/messages') %>
      <h3 class="text-dark mb-4">Manage Users</h3>
      <div class="card shadow">
        <div class="card-header py-3">
          <p class="text-primary m-0 font-weight-bold">Users Info</p>
        </div>
        <div class="card-body">

          <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
            <table class="table my-0" id="tblCallLogs">
              <thead>
                <tr>
                  <th scope="col">#</th>
                  <th scope="col">First Name</th>
                  <th scope="col">Last Name</th>
                  <th scope="col">UserName</th>
                  <th scope="col">email</th>
                  <th scope="col">MSISDN</th>
                  <th scope="col">User Level</th>
                  <th scope="col" colspan="2">Action</th>
                </tr>
              </thead>
              <tbody>
                <% if (typeof users !='undefined' ) { %>
                <% for( let index=0; index < users.length; index++ ) { %>
                <tr>
                  <td scope="row">
                    <%- (index+1) %>
                  </td>
                  <td>
                    <%- users[index].firstName %>
                  </td>
                  <td>
                    <%- users[index].lastName %>
                  </td>
                  <td>
                    <%- users[index].username %>
                  </td>
                  <td>
                    <%- users[index].email %>
                  </td>
                  <td>
                    <%- users[index].number %>
                  </td>
                  <td>
                    <%- users[index].levelName %>
                  </td>
                  <td scope="row"><a href="javascript:void(0)" onclick="location.href='/admin/admin-dashboard/delete/<%- users[index].userId %>'" class="link text-danger">
                      <i class="fa fa-trash text-danger text-center fa-lg" aria-hidden="true"></i></a>
                  </td>

                </tr>


                <% } %>
                <% } %>


              </tbody>
              <tfoot>
                <tr>

                </tr>
              </tfoot>
            </table>
          </div>
          
        </div>
      </div>
