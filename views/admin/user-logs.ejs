                        <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                            <table class="table my-0" id="audittrail">
                                <thead>
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">User Name</th>
                                        <th scope="col">User Level</th>
                                        <th scope="col">IP Address</th>
                                        <th scope="col">User Agent</th>
                                        <th scope="col">Method</th>
                                        <th scope="col">URL</th>
                                        <th scope="col">Date/ Time</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (typeof audits !='undefined' ) { %>
                                        <% for( let index=0; index < audits.length; index++ ) { %>
                                    <tr>
                                        <td scope="row">
                                            <%- (index+1) %>
                                        </td>
                                        <td>
                                            <%- audits[index].username %>
                                        </td>
                                        <td>
                                            <%- audits[index].levelName %>
                                        </td>
                                        <td>
                                            <%- audits[index].ipaddress %>
                                        </td>
                                        <td>
                                            <%- audits[index].useragent %>
                                        </td>
                                        <td>
                                            <%- audits[index].method %>
                                        </td>
                                        <td>
                                            <%- audits[index].url %>
                                        </td>
                                        <td>
                                            <%- audits[index].datetime %>
                                        </td>
                                       
                                    </tr>
                                    
                                    
