                                        <td><%= log.CALL_TERMINATING_NUMBER %> </td>
                                        <td><%= log.ZONE_NAME %> </td>
                                        <td><%= log.SITE_NAME %> </td>
                                    </tr>
                                     <% }) %>
                                      <% } %> 
